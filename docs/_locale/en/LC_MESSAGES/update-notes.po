msgid "*Windows:*"
msgstr "*Windows:*"

#: ../../update-notes.rst:107
msgid ""
"``$MONGO_HOME\\bin\\mongorestore.exe -u effektif -p <password> --"
"authenticationDatabase admin --db effektif C:\\path\\to\\dumps\\effektif``"
msgstr ""
"``$MONGO_HOME\\bin\\mongorestore.exe -u effektif -p <password> --"
"authenticationDatabase admin --db effektif C:\\path\\to\\dumps\\effektif``"

